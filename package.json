{
  "name": "@jawang94/github-pr-release",
  "version": "1.4.2",
  "description": "Create a release pull request by Github API",
  "main": "index.js",
  "bin": {
    "github-pr-release": "cli/index.js"
  },
  "author": "Jason Wang",
  "contributors": [
    "Kazato Sugimoto"
  ],
  "license": "MIT",
  "scripts": {
