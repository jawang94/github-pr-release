  "repository": {
    "type": "git",
    "url": "https://github.com/jawang94/github-pr-release"
  },
  "dependencies": {
    "es6-promise": "^2.3.0",
    "moment": "^2.9.0",
    "mustache": "^3.0.1",
    "parse-link-header": "^2.0.0",
    "request": "^2.58.0",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "espower-loader": "^1.2.2",
    "husky": "^7.0.4",
    "intelli-espower-loader": "^0.7.0",
    "lint-staged": "^12.2.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.5",
    "power-assert": "^0.11.0",
    "prettier": "2.5.1",
    "proxyquire": "^1.7.2",
    "release-it": "^14.12.3"
  },
  "release-it": {
    "github": {
